    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="HelloWorldService">
    <wsdl:port name="HelloWorldServiceSOAP" binding="tns:HelloWorldServiceSOAP">
      <soap:address location="http://localhost:8080/axis2/services/HelloWorldService"/>
    </wsdl:port>
  </wsdl:service>
